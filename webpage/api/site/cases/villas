ien","post_excerpt":"","post_status":"publish","comment_status":"closed","ping_status":"closed","post_password":"","post_name":"erwin-van-strien","to_ping":"","pinged":"","post_modified":"2020-12-17 16:08:06","post_modified_gmt":"2020-12-17 16:08:06","post_content_filtered":"","post_parent":0,"guid":"http://admin.powerhouse.buildin.amsterdam/?post_type=team&#038;p=519","menu_order":39,"post_type":"team","post_mime_type":"","comment_count":"0","filter":"raw"},{"ID":1052,"post_author":"3","post_date":"2019-10-17 13:08:25","post_date_gmt":"2019-10-17 13:08:25","post_content":"","post_title":"Justin Tjon Kie Sim","post_excerpt":"","post_status":"publish","comment_status":"closed","ping_status":"closed","post_password":"","post_name":"1052","to_ping":"","pinged":"","post_modified":"2020-09-09 09:49:51","post_modified_gmt":"2020-09-09 09:49:51","post_content_filtered":"","post_parent":0,"guid":"http://admin.powerhouse.buildin.amsterdam/?post_type=team&#038;p=1052","menu_order":55,"post_type":"team","post_mime_type":"","comment_count":"0","filter":"raw"}],"partner_in_charge_name":"","project_lead_names":false,"project_team_names":[{"name":"Melissa Fourie"},{"name":"Severino Iritano"},{"name":"Marah Wagner"},{"name":"Beyza Uyar"}]},"collaborator_groups":[{"category":{"category":"Project management","custom_category":""},"collaborators":[{"name":"DVP","url":"https://www.dvp.nl/"}]},{"category":{"category":"Contractor","custom_category":""},"collaborators":[{"name":"FOR Building VOF","url":"https://www.forbuilding.nl/"}]},{"category":{"category":"","custom_category":"Floating foundation"},"collaborators":[{"name":"Hercules FC","url":"https://herculesfc.nl/en/projects/"}]},{"category":{"category":"Structural engineer","custom_category":""},"collaborators":[{"name":"Bartels & Vedder","url":"https://www.bartelsvedder.nl/?lang=en"}]},{"category":{"category":"","custom_category":"Wood structure engineer"},"collaborators":[{"name":"Solid Timber","url":"https://solidtimber.nl/"}]},{